{
  "name": "express_boilerplate",
  "version": "0.0.1",
  "description": "A simple express app boilerplate for starting new projects",
  "main": "index.js",
  "author": "Javier Alonso Gutiérrez Sepúlveda",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/javier-gs00/express_boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/javier-gs00/express_boilerplate"
  },
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "build": "rm -rf dist && babel src --out-dir dist",
    "serve": "node dist/index.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0"
  },
  "engines": {
    "node": ">=8.x",
    "npm": ">=5.x",
    "yarn": ">=1.7.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-plugin-module-resolver": "^3.1.1"
  }
}
