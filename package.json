{
  "name": "express_boilerplate",
  "version": "0.0.1",
  "description": "A simple express app boilerplate for starting new projects",
  "main": "index.js",
  "author": "Javier Alonso Gutiérrez Sepúlveda",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/javier-gs00/express_boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/javier-gs00/express_boilerplate"
  },
  "scripts": {
    "start:dev": "./node_modules/nodemon/bin/nodemon.js --exec babel-node src/index.js",
    "start:prod": "node dist/index.js",
    "build": "rm -rf dist && babel src --out-dir dist",
    "test": "jest --watch"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.16.0",
    "morgan": "^1.9.0"
  },
  "engines": {
    "node": ">=8.x",
    "npm": ">=5.x",
    "yarn": ">=1.7.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "jest": "^23.6.0",
    "nodemon": "^1.18.10",
    "regenerator-runtime": "^0.13.1",
    "supertest": "^3.4.2"
  }
}
